
{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["dist/**"] }
    },
    {
      "src": "dist/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
  
    {
      "src": "/(auth|cart|contact|other-backend-routes)(.*)?",
      "dest": "server.js"
    },
   
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
